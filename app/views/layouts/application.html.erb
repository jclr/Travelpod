<!DOCTYPE html>
<html>
<head>
  <title>Travelpod</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
    <%= link_to "Users", users_path, class: 'btn btn-success' %>
    <%= link_to "Trips", trips_path, class: 'btn btn-success' %>
    <%= link_to "Cities", cities_path, class: 'btn btn-success' %>
</div>

<div class="container">
    <%= yield %>
</div>

</body>
</html>
